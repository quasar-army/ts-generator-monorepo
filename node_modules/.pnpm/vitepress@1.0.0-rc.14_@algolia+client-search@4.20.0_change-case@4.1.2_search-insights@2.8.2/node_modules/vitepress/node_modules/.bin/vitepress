#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules/vitepress/bin/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules/vitepress/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules/vitepress/bin/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules/vitepress/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.14_@algolia+client-search@4.20.0_change-case@4.1.2_search-insights@2.8.2/node_modules:/home/luke/code/ts-generator-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
